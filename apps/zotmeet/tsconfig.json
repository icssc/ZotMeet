{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "bundler",
    "plugins": [
      {
        "name": "typescript-svelte-plugin"
      }
    ]
  },
  "include": ["src", "tools"]
}
