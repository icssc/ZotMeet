{
  "name": "zotmeet",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "vite build",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "db:pull": "drizzle-kit db:pull",
    "db:push": "drizzle-kit db:push",
    "db:update": "pnpm generate && pnpm migrate",
    "dev": "sst bind vite dev",
    "format": "prettier --write .",
    "generate": "drizzle-kit generate",
    "postinstall": "husky install && pnpm generate && svelte-kit sync",
    "lint": "eslint --quiet --fix .",
    "migrate": "pnpm tsx src/lib/db/migrate.ts",
    "migrate:dev": "pnpm dotenv -- pnpm tsx src/lib/db/migrate.ts",
    "preview": "vite preview",
    "sst:deploy": "sst deploy",
    "sst:dev": "sst dev",
    "start": "vite",
    "studio": "drizzle-kit studio",
    "test": "pnpm test:integration && pnpm test:unit",
    "test:integration": "playwright test",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@iconify/json": "^2.2.208",
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "arctic": "^1.2.1",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.1",
    "drizzle-orm": "0.29.5",
    "lucia": "^3.1.1",
    "pg": "^8.11.3",
    "postgres": "3.4.3",
    "svelty-picker": "^5.2.7",
    "tailwind-merge": "^2.2.1",
    "tsx": "^4.7.2",
    "unplugin-icons": "^0.18.5"
  },
  "devDependencies": {
    "@commitlint/cli": "18.4.3",
    "@commitlint/config-conventional": "18.4.3",
    "@commitlint/types": "18.4.3",
    "@playwright/test": "1.40.0",
    "@skeletonlabs/skeleton": "^2.10.0",
    "@skeletonlabs/tw-plugin": "^0.2.4",
    "@sveltejs/adapter-auto": "2.1.1",
    "@sveltejs/kit": "1.27.6",
    "@tailwindcss/forms": "^0.5.7",
    "@types/lint-staged": "13.2.2",
    "@types/node": "20.10.0",
    "@types/pg": "^8.11.2",
    "@typescript-eslint/eslint-plugin": "6.12.0",
    "@typescript-eslint/parser": "6.12.0",
    "autoprefixer": "^10.4.19",
    "aws-cdk-lib": "2.110.1",
    "constructs": "10.3.0",
    "cz-conventional-changelog": "3.3.0",
    "daisyui": "^4.11.1",
    "devmoji": "2.3.0",
    "dotenv": "^16.4.5",
    "drizzle-kit": "0.21.1",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-svelte": "2.35.1",
    "husky": "8.0.3",
    "lint-staged": "15.1.0",
    "pg": "^8.11.3",
    "postcss": "^8.4.38",
    "postcss-load-config": "^4.0.2",
    "prettier": "3.1.0",
    "prettier-plugin-packagejson": "2.4.6",
    "prettier-plugin-svelte": "3.1.2",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "sst": "2.36.6",
    "svelte": "4.2.7",
    "svelte-check": "3.6.2",
    "svelte-kit-sst": "2.36.6",
    "sveltekit-superforms": "^1.13.4",
    "tailwindcss": "^3.4.3",
    "tslib": "2.6.2",
    "typescript": "5.3.2",
    "vite": "5.0.2",
    "vitest": "0.34.6",
    "zod": "^3.22.4"
  }
}
