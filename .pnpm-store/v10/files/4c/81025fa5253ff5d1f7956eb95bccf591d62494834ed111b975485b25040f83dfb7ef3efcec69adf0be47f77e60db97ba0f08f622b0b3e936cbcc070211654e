{"version":3,"file":"compare-Br3z3FUS.js","names":[],"sources":["../src/lib/compare.ts"],"sourcesContent":["import type { Query } from './search-params'\n\nexport function compareQuery<T extends Query>(\n  a: T | null,\n  b: T | null\n): boolean {\n  if (a === b) {\n    return true // Referentially stable\n  }\n  if (a === null || b === null) {\n    return false\n  }\n  // we expect either strings or arrays, not a mix of both\n  if (typeof a === 'string' || typeof b === 'string') {\n    return false\n  }\n\n  if (a.length !== b.length) {\n    return false\n  }\n\n  return a.every((value, index) => value === b[index]!)\n}\n"],"mappings":";AAEA,SAAgB,aACd,GACA,GACS;AACT,KAAI,MAAM,EACR,QAAO;AAET,KAAI,MAAM,QAAQ,MAAM,KACtB,QAAO;AAGT,KAAI,OAAO,MAAM,YAAY,OAAO,MAAM,SACxC,QAAO;AAGT,KAAI,EAAE,WAAW,EAAE,OACjB,QAAO;AAGT,QAAO,EAAE,OAAO,OAAO,UAAU,UAAU,EAAE,OAAQ"}