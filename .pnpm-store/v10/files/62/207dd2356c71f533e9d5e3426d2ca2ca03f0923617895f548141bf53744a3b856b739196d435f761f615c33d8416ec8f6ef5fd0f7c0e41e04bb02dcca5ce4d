import { Check } from "./checks.cjs";
import { ForeignKey } from "./foreign-keys.cjs";
import { GelPolicy } from "./policies.cjs";
import { PrimaryKey } from "./primary-keys.cjs";
import { UniqueConstraint } from "./unique-constraint.cjs";
import { GelTable } from "./table.cjs";
import { Index } from "./indexes.cjs";
import { AnyGelColumn, GelColumn } from "./columns/common.cjs";
import "./columns/index.cjs";
import { GelViewBase } from "./view-base.cjs";
import { GelMaterializedView, GelMaterializedViewWithConfig, GelView, ViewWithConfig } from "./view.cjs";
import "./index.cjs";
import * as __sql_sql_ts3 from "../sql/sql.cjs";
import { SQL } from "../sql/sql.cjs";
import { Subquery } from "../subquery.cjs";

//#region src/gel-core/utils.d.ts
declare function getTableConfig<TTable extends GelTable>(table: TTable): {
  columns: GelColumn<any, {}>[];
  indexes: Index[];
  foreignKeys: ForeignKey[];
  checks: Check[];
  primaryKeys: PrimaryKey[];
  uniqueConstraints: UniqueConstraint[];
  name: string;
  schema: string | undefined;
  policies: GelPolicy[];
  enableRLS: boolean;
};
declare function extractUsedTable(table: GelTable | Subquery | GelViewBase | SQL): string[];
declare function getViewConfig<TName extends string = string, TExisting extends boolean = boolean>(view: GelView<TName, TExisting>): {
  with?: ViewWithConfig;
  name: TName;
  originalName: TName;
  schema: string | undefined;
  selectedFields: __sql_sql_ts3.ColumnsSelection;
  isExisting: TExisting;
  query: TExisting extends true ? undefined : SQL<unknown>;
  isAlias: boolean;
};
declare function getMaterializedViewConfig<TName extends string = string, TExisting extends boolean = boolean>(view: GelMaterializedView<TName, TExisting>): {
  with?: GelMaterializedViewWithConfig;
  using?: string;
  tablespace?: string;
  withNoData?: boolean;
  name: TName;
  originalName: TName;
  schema: string | undefined;
  selectedFields: __sql_sql_ts3.ColumnsSelection;
  isExisting: TExisting;
  query: TExisting extends true ? undefined : SQL<unknown>;
  isAlias: boolean;
};
type ColumnsWithTable<TTableName extends string, TForeignTableName extends string, TColumns extends AnyGelColumn<{
  tableName: TTableName;
}>[]> = { [Key in keyof TColumns]: AnyGelColumn<{
  tableName: TForeignTableName;
}> };
//#endregion
export { ColumnsWithTable, extractUsedTable, getMaterializedViewConfig, getTableConfig, getViewConfig };
//# sourceMappingURL=utils.d.cts.map