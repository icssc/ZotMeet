import { entityKind } from "../entity.js";
import { TaggedError } from "effect/Data";

//#region src/effect-core/errors.ts
var TaggedDrizzleError = class extends TaggedError("DrizzleError") {
	static [entityKind] = "TaggedDrizzleError";
	message;
	cause;
	constructor({ message, cause }) {
		super();
		this.name = "TaggedDrizzleError";
		this.message = message ?? "";
		this.cause = cause;
	}
};
var TaggedDrizzleQueryError = class TaggedDrizzleQueryError extends TaggedError("DrizzleQueryError") {
	static [entityKind] = "TaggedDrizzleQueryError";
	message;
	constructor(query, params, cause) {
		super();
		this.query = query;
		this.params = params;
		this.cause = cause;
		this.message = `Failed query: ${query}\nparams: ${params}`;
		Error.captureStackTrace(this, TaggedDrizzleQueryError);
		if (cause) this.cause = cause;
	}
};
var TaggedTransactionRollbackError = class extends TaggedError("TransactionRollbackError") {
	static [entityKind] = "TaggedTransactionRollbackError";
	message = "Rollback";
};

//#endregion
export { TaggedDrizzleError, TaggedDrizzleQueryError, TaggedTransactionRollbackError };
//# sourceMappingURL=errors.js.map