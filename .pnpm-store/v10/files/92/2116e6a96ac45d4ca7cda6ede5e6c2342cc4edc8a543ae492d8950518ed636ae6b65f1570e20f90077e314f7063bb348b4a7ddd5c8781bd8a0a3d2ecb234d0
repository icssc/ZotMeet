const require_rolldown_runtime = require('../_virtual/rolldown_runtime.cjs');
let __entity_ts = require("../entity.cjs");
let effect_Data = require("effect/Data");

//#region src/effect-core/errors.ts
var TaggedDrizzleError = class extends (0, effect_Data.TaggedError)("DrizzleError") {
	static [__entity_ts.entityKind] = "TaggedDrizzleError";
	message;
	cause;
	constructor({ message, cause }) {
		super();
		this.name = "TaggedDrizzleError";
		this.message = message ?? "";
		this.cause = cause;
	}
};
var TaggedDrizzleQueryError = class TaggedDrizzleQueryError extends (0, effect_Data.TaggedError)("DrizzleQueryError") {
	static [__entity_ts.entityKind] = "TaggedDrizzleQueryError";
	message;
	constructor(query, params, cause) {
		super();
		this.query = query;
		this.params = params;
		this.cause = cause;
		this.message = `Failed query: ${query}\nparams: ${params}`;
		Error.captureStackTrace(this, TaggedDrizzleQueryError);
		if (cause) this.cause = cause;
	}
};
var TaggedTransactionRollbackError = class extends (0, effect_Data.TaggedError)("TransactionRollbackError") {
	static [__entity_ts.entityKind] = "TaggedTransactionRollbackError";
	message = "Rollback";
};

//#endregion
exports.TaggedDrizzleError = TaggedDrizzleError;
exports.TaggedDrizzleQueryError = TaggedDrizzleQueryError;
exports.TaggedTransactionRollbackError = TaggedTransactionRollbackError;
//# sourceMappingURL=errors.cjs.map