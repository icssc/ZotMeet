//#region src/migrator.d.ts
interface KitConfig {
  out: string;
  schema: string;
}
interface MigrationConfig {
  migrationsFolder: string;
  migrationsTable?: string;
  migrationsSchema?: string;
}
interface MigrationMeta {
  sql: string[];
  folderMillis: number;
  hash: string;
  bps: boolean;
}
interface MigrationFromJournalConfig {
  migrationsJournal: MigrationsJournal;
  migrationsTable?: string;
}
type MigrationsJournal = {
  sql: string;
  timestamp: number;
}[];
/** Only gets returned if migrator failed with `init: true` used by `drizzle-kit pull --init`*/
interface MigratorInitFailResponse {
  exitCode: 'databaseMigrations' | 'localMigrations';
}
/** Only gets returned if migrator failed with `init: true` used by `drizzle-kit pull --init`*/
interface MigratorInitFailResponse {
  exitCode: 'databaseMigrations' | 'localMigrations';
}
declare function readMigrationFiles(config: MigrationConfig): MigrationMeta[];
//#endregion
export { KitConfig, MigrationConfig, MigrationFromJournalConfig, MigrationMeta, MigrationsJournal, MigratorInitFailResponse, readMigrationFiles };
