> primary.delete(...args);\n\tconst execute: Q['execute'] = (...args: [any]) => primary.execute(...args);\n\tconst transaction: Q['transaction'] = (...args: [any, any]) => primary.transaction(...args);\n\n\treturn {\n\t\t...primary,\n\t\tupdate,\n\t\tinsert,\n\t\tdelete: $delete,\n\t\texecute,\n\t\ttransaction,\n\t\t$primary: primary,\n\t\t$replicas: replicas,\n\t\tselect,\n\t\tselectDistinct,\n\t\twith: $with,\n\t\tget _query() {\n\t\t\treturn getReplica(replicas)._query;\n\t\t},\n\t};\n};\n"],"mappings":";;;;;;;;;;;;;AA4BA,IAAa,gBAAb,MAKE;CACD,QAAiBA,0BAAsB;CAOvC;CAMA,YAEC,AAASC,SAET,AAASC,SACT,QACC;EAJQ;EAEA;AAGT,OAAK,IAAI,SACN;GAAE,QAAQ,OAAO;GAAQ,eAAe,OAAO;GAAe,GAC9D;GAAE,QAAQ;GAAW,eAAe,EAAE;GAAE;AAC3C,OAAK,SAAS,EAAE;AAChB,MAAI,KAAK,EAAE,OACV,MAAK,MAAM,CAAC,WAAW,YAAY,OAAO,QAAQ,KAAK,EAAE,OAAO,CAC/D,CAAC,KAAK,OAAyF,aAC9F,IAAIC,+DACH,OAAQ,YACR,KAAK,EAAE,QACP,KAAK,EAAE,eACP,OAAQ,WAAW,YACnB,SACA,SACA,QACA;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;CAqCL,MAA6B,OAAe;EAC3C,MAAM,OAAO;AAEb,SAAO,EACN,GACC,IACgD;AAChD,OAAI,OAAO,OAAO,WACjB,MAAK,GAAG,IAAIC,6DAAa,KAAK,QAAQ,CAAC;AAGxC,UAAO,IAAI,MACV,IAAIC,2BAAa,GAAG,QAAQ,EAAE,GAAG,mBAAmB,EAAoB,OAAO,KAAK,EACpF,IAAIC,2CAAsB;IAAE;IAAO,oBAAoB;IAAS,aAAa;IAAS,CAAC,CACvF;KAEF;;;;;;;;;;;;;;;;;;;;;CAsBF,KAAK,GAAG,SAAyB;EAChC,MAAM,OAAO;EAMb,SAAS,OAAO,QAA4F;AAC3G,UAAO,IAAIC,4DAAmB;IAC7B,QAAQ,UAAU;IAClB,SAAS,KAAK;IACd,SAAS,KAAK;IACd,UAAU;IACV,CAAC;;EAOH,SAAS,eACR,QACoE;AACpE,UAAO,IAAIA,4DAAmB;IAC7B,QAAQ,UAAU;IAClB,SAAS,KAAK;IACd,SAAS,KAAK;IACd,UAAU;IACV,UAAU;IACV,CAAC;;AAGH,SAAO;GAAE;GAAQ;GAAgB;;CAyClC,OAAO,QAA4F;AAClG,SAAO,IAAIA,4DAAmB;GAAE,QAAQ,UAAU;GAAW,SAAS,KAAK;GAAS,SAAS,KAAK;GAAS,CAAC;;CA+B7G,eAAe,QAA4F;AAC1G,SAAO,IAAIA,4DAAmB;GAC7B,QAAQ,UAAU;GAClB,SAAS,KAAK;GACd,SAAS,KAAK;GACd,UAAU;GACV,CAAC;;;;;;;;;;;;;;;;;;;;;;;CAwBH,OAAkC,OAA4E;AAC7G,SAAO,IAAIC,4DAAmB,OAAO,KAAK,SAAS,KAAK,QAAQ;;;;;;;;;;;;;;;;;;;;;CAsBjE,OAAkC,OAA4E;AAC7G,SAAO,IAAIC,4DAAmB,OAAO,KAAK,SAAS,KAAK,QAAQ;;;;;;;;;;;;;;;;;;;;;CAsBjE,OAAkC,OAAyE;AAC1G,SAAO,IAAIC,yDAAgB,OAAO,KAAK,SAAS,